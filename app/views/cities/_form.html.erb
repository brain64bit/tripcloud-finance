<%= simple_form_for @city, html: { class: "form-horizontal"} do |f| %>
	<%= f.input :name, input_html: { class: "span6 m-wrap" } %>
	<%= f.input :airport_name, input_html: { class: "span6 m-wrap" } %>
	<%= f.input :code, input_html: { class: "span6 m-wrap" } %>	
	<%= f.input :country, as: :string, input_html: { class: "span6 m-wrap" } %>	
	<div class="form-actions">
		<%= f.button :submit, "Submit", class: "btn blue" %>
	</div>
<% end %>